<h1><%= @contact.name %></h1>

<p>Phone: <%= @contact.phone %></p>
<p>Email: <%= @contact.email %></p>

<p><%= link_to "Edit", edit_contact_path(@contact) %></p>
<p><%= link_to "Delete", contact_path(@contact),
                         :confirm => "You sure",
                         :method => "delete" %></p>
<p><%= link_to "Homepage", contacts_path %></p>
